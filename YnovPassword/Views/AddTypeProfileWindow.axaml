<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="YnovPassword.Views.AddTypeProfileWindow"
        Width="300" Height="150"
        Title="Ajouter un type de profil">
    <StackPanel Margin="10" Spacing="10">
        <TextBlock Text="Nom du nouveau type de profil:" />
        <TextBox x:Name="txtNewTypeProfile" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Spacing="10">
            <Button Content="OK" Click="BtnOk_Click" />
            <Button Content="Annuler" Click="BtnCancel_Click" />
        </StackPanel>
    </StackPanel>
</Window>
